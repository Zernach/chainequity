{
  "expo": {
    "name": "ChainEquity",
    "slug": "chainequity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chainequity-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "chainequity",
    "splash": {
      "image": "./assets/chainequity-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.archlife.peaksix-chainequity",
      "associatedDomains": [
        "applinks:peaksix.chainequity.archlife.org",
        "applinks:*.peaksix.chainequity.archlife.org"
      ],
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "phantom",
          "solflare",
          "backpack",
          "glow",
          "slope",
          "sollet",
          "trust",
          "wc"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/chainequity-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "org.archlife.peaksix-chainequity",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "chainequity"
            },
            {
              "scheme": "https",
              "host": "peaksix.chainequity.archlife.org",
              "pathPrefix": "/wc"
            }
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/chainequity-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
